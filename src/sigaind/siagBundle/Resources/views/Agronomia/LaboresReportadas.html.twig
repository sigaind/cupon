{% extends 'siagBundle::layoutMaestra.html.twig' %}

{% block title %}Labores Reportadas{% endblock %}

{% block head %}
<link href="{{ asset('bundles/sonatajquery/themes/base/jquery.ui.theme.css') }}" rel="stylesheet" type="text/css" />
<link href="{{ asset('bundles/sonatajquery/themes/base/jquery.ui.datepicker.css') }}" rel="stylesheet" type="text/css" />
<script src="{{ asset('bundles/sigaind/js/agronomia/LaboresReportadas.js') }}" type="text/javascript"></script>
<script type="text/javascript">
  $(document).ready(function() {
       inicio()     
  });
</script>
{% endblock %}
    
{% block content %}
<div id="consulta" >

    <table id='demotable1' class="tablaFiltro">      
      <tbody id="tblFormConsulta">        
            <tr>
                    <td> Fecha Inicial: </td>
                    <td>
                        <input type="text" id="f1" />
                    </td>
            </tr>
            <tr>
                    <td> Fecha Final: </td>
                    <td> 
                        <input type="text" id="f2" />
                    </td>
            </tr>
      <tbody>   
  </table> 
</div>
<div id="botonConsulta">
    <button type="button" onclick="consultarLabores()">Aceptar</button>
    <button type="button">Cancelar</button>
</div>

<div id="divTabla" style="display: none">
    <div id="info"></div>        
</div>
<div id="botonTabla" style="display: none">
    <button type="button" onclick="cancelarTablaLabores()">Cancelar</button>
    Exportar a: <a href="#" onclick="exportarExcel()">Excel</a>, <a href="#" onclick="exportarPDF()">PDF.</a>    
</div>

<div id="cargando" style="display: none">
    <img src="{{ asset('bundles/sigaind/images/loader.gif') }}" alt="Cargando.." />
</div>
  
{% endblock %}
