{% extends 'siagBundle::layoutMaestra.html.twig' %}

{% block title %}Programacion de Labores{% endblock %}

{% block head %}
<link href="{{ asset('bundles/sonatajquery/themes/base/jquery.ui.theme.css') }}" rel="stylesheet" type="text/css" />
<link href="{{ asset('bundles/sonatajquery/themes/base/jquery.ui.datepicker.css') }}" rel="stylesheet" type="text/css" />
<script src="{{ asset('bundles/sigaind/js/Informes/Agronomia/LaboresProgramadas.js') }}" type="text/javascript"></script>
<script src="{{ asset('bundles/sigaind/js/Informes/Agronomia/construirXML.js') }}" type="text/javascript"></script>

<script language="JavaScript" src="{{ asset('bundles/sigaind/FusionChartsFree/JSClass/FusionCharts.js') }}"></script>

<script type="text/javascript">
  $(document).ready(function() {
       inicio()     
  });
</script>
{% endblock %}
    
{% block content %}
<div id="consulta" >

    <table id='demotable1' class="tablaFiltro">      
      <tbody id="tblFormConsulta">
            <tr>
                <td colspan="2"> LABORES PROGRAMADAS </td>
            </tr>
            <tr>
                    <td> Fecha Inicial: </td>
                    <td>
                        <input type="text" id="f1" />
                    </td>
            </tr>
            <tr>
                    <td> Fecha Final: </td>
                    <td> 
                        <input type="text" id="f2" />
                    </td>
            </tr>
      <tbody>   
  </table> 
</div>
<div id="botonConsulta">
    <button type="button" onclick="consultarProgramacion()">Aceptar</button>
    <button type="button">Cancelar</button>
</div>

<div id="divTabla" style="display: none">
    <table width="98%" border="0" cellspacing="0" cellpadding="3" align="center">
        <tr> 
            <td valign="top" class="text" align="center"> 
                <div id="chartdiv" align="center"> FusionCharts. </div>
            </td>
        </tr>
        <tr>
            <td valign="top" class="text" align="center">&nbsp;</td>
        </tr>  
    </table>
</div>

<div id="cargando" style="display: none">
    <img src="{{ asset('bundles/sigaind/images/loader.gif') }}" alt="Cargando.." />
</div>
  
{% endblock %}
